{
  "name": "sorrowblue.github.io",
  "version": "1.0.0",
  "description": "SorrowBlueHome build by Nuxt.js.",
  "author": "SorrowBlue",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "md": "processmd content/blog/**/*.md --summaryOutput static/content/blog/summary.json --outputDir static/content/blog/json --markdownOptions.html=true --markdownOptions.linkify=true"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.5.4",
    "@nuxtjs/pwa": "^2.6.0",
    "node-sass": "^4.12.0",
    "nuxt": "^2.8.1",
    "sass-loader": "^7.1.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^0.2.1",
    "@nuxtjs/vuetify": "^1.0.1",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-nuxt": ">=0.4.3",
    "eslint-plugin-prettier": "^3.1.0",
    "json-loader": "^0.5.7",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "processmd": "^4.4.2"
  }
}
